<h1>Listing servicios</h1>

<table>
  <tr>
    <th>Nombre</th>

  </tr>

<% @servicios.each do |servicios| %>
  <tr>
    <td><%=h servicios.nombre %></td>

    <td><%= link_to 'Show', servicios %></td>
    <td><%= link_to 'Edit', edit_servicio_path(servicios) %></td>
    <td><%= link_to 'Destroy', servicios, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New servicios', new_servicio_path %>
